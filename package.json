{
  "name": "flatup-gym-website",
  "version": "1.0.0",
  "description": "FLAT-UP GYM - 世界一優しい格闘技ジム公式ウェブサイト",
  "keywords": [
    "キックボクシング",
    "格闘技",
    "ジム",
    "成田市",
    "女性",
    "キッズ",
    "シニア",
    "産後",
    "ダイエット"
  ],
  "author": "FLAT-UP GYM",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -H 0.0.0.0 -p 3000",
    "build": "next build && node scripts/critical.js",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "export": "next export",
    "start": "next start",
    "start:production": "cross-env NODE_ENV=production npm run start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true npm run build",
    "sitemap": "next-sitemap",
    "sitemap:generate": "npm run build && npm run sitemap",
    "check-links": "http-server ./out -p 8081 & PID=$! && npx blc http://localhost:8081 -r -o && kill $PID",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "optimize": "npm run lint:fix && npm run type-check && npm run build:production",
    "performance": "npm run build:analyze && npm run test:coverage",
    "deploy": "npm run build:production && npm run sitemap:generate",
    "optimize-images": "node scripts/optimize-images.js",
    "partytown": "partytown copylib public/~partytown"
  },
  "dependencies": {
    "@qwik.dev/partytown": "^0.11.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.542.0",
    "next": "15.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "broken-link-checker": "^0.7.8",
    "critical": "^7.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.3.2",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "netlify-cli": "^23.5.1",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/flatupgym/website.git"
  },
  "bugs": {
    "url": "https://github.com/flatupgym/website/issues"
  },
  "homepage": "https://flat-up.jp"
}